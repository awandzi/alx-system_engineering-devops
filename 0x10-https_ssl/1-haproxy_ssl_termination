 log /dev/log    localo
        log /dev/log    locali notice
        chroot /var/lib/haproxy
        stats socket /run/haproxy/admin.sock mode 660levelamin expose.fd listeners
        stats timeout 30s
        user haproxy
        group haproxy
        daemon

        # default ssl
        ca-base /etc/ssl/certs
        crt-base /etc/ssl/pivate
        # see: https://ssl-config.mozilla.org

